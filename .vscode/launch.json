  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug API Gateway",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless",
      "env": {
        "AWS_PROFILE": "xxxxxx",
        "AWS_REGION": "us-east-1",
        "Region": "us-east-1",
        "AccountId": "xxxxxx",
        //"IS_OFFLINE": "false",
      },
      "args": [
        "offline",
        "start",
        "--stage",
        "dev"
      ],
      "outFiles": [
        "${workspaceFolder}/.webpack/**/*.js"
      ]
    }
  ]
}