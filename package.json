{
  "name": "serverless-utility-platform",
  "version": "1.0.0",
  "description": "This serverless platform is used to create/re-recreate service as needed for testing or troubleshooting purposes.",
  "main": "index.js",
  "author": "Tri Luong",
  "license": "MIT",
  "scripts": {
    "sls": "node_modules/.bin/sls",
    "test": "node_modules/.bin/mocha \"./{,!(node_modules)/**/}*.spec.ts\" -t 10000 --require ts-node/register --require mocha.env --require tsconfig-paths/register --reporter spec",
    "test-plugins": "node_modules/.bin/mocha .serverless_plugins/**/*.spec.js -t 10000 --require mocha.env --reporter spec",
    "coverage": "node_modules/.bin/nyc --reporter cobertura  --reporter lcov --reporter text --reporter text-summary node_modules/.bin/mocha \"./{,!(node_modules)/**/}*.spec.ts\" -t 10000 --require ts-node/register --require mocha.env  --require tsconfig-paths/register --reporter spec",
    "junitReport": "node_modules/.bin/mocha \"./{,!(node_modules)/**/}*.spec.ts\" -t 10000 --require ts-node/register --require tsconfig-paths/register --require mocha.env --reporter mocha-junit-reporter",
    "lint": "node_modules/.bin/tslint --project . ",
    "lint-plugins": "node_modules/.bin/tslint ./.serverless_plugins/validate-variable-resolution/index.js",
    "package": "node_modules/.bin/sls package --stage dev",
    "run-offline": "node_modules/.bin/sls offline start --stage dev",
    "generate-documentation": "node_modules/.bin/sls openapi generate --stage dev -o Swagger/swagger-ui-3.20.1/dist/jettatorOrder-spec.yml -f yaml",
    "tsjson-get-request": "typescript-json-schema ./src/domain/Order/BaseOrder.ts IWSGOrder -o ./apiDocs/wsgOrderModel.json --titles --required --ignoreErrors",
    "tsjson-get-response": "node_modules/.bin/typescript-json-schema ./src/domain/Response/JettatorResponse.ts IOrderResults -o ./apiDocs/responseModel.json --ignoreErrors",
    "tsjson-get-output-request": "typescript-json-schema ./src/domain/Transaction/BaseTransaction.ts ITransaction -o ./apiDocs/transactionOrderModel.json --titles --required --ignoreErrors",
    "tsjson-get-temp-request": "typescript-json-schema ./src/domain/Order/BaseOrder.ts IWSGOrder -o ./apiDocs/wsgTempOrderModel.json --titles --required --ignoreErrors",
    "tsjson-get-temp-response": "node_modules/.bin/typescript-json-schema ./src/domain/Response/JettatorResponse.ts IOrderResults -o ./apiDocs/responseTempModel.json --ignoreErrors",
    "tsjson-get-temp-output-request": "typescript-json-schema ./src/domain/Transaction/BaseTransaction.ts ITransaction -o ./apiDocs/transactionTempOrderModel.json --titles --required --ignoreErrors",
    "tsjson-get-all": "typescript-json-schema ./src/domain/Order/BaseOrder.ts IWSGOrder -o ./apiDocs/wsgOrderModel.json --titles --required --ignoreErrors && node_modules/.bin/typescript-json-schema ./src/domain/Response/JettatorResponse.ts IOrderResults -o ./apiDocs/responseModel.json --ignoreErrors && typescript-json-schema ./src/domain/Transaction/BaseTransaction.ts ITransaction -o ./apiDocs/transactionOrderModel.json --titles --required --ignoreErrors",
    "deploy-dev": "node_modules/.bin/sls deploy --stage dev",
    "deploy-qa": "node_modules/.bin/sls deploy --stage qa",
    "deploy-ba": "node_modules/.bin/sls deploy --stage ba",
    "deploy-prod": "node_modules/.bin/sls deploy --stage prod",
    "clean": "rm test-results.xml & rm -rf coverage & rm -rf .nyc_output & rm -rf .serverless & rm yarn-error.log & rm apiDocs/*.*",
    "install-dynamo": "node_modules/.bin/sls dynamodb install --stage dev > ddbinstall.txt",
    "start-dynamo": "node_modules/.bin/sls dynamodb start",
    "local-dynamo": "java -Djava.library.path=./.dynamodb/DynamoDBLocal_lib -jar ./.dynamodb/DynamoDBLocal.jar",
    "coveragejs": "node coverage.js"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@types/aws-lambda": "^8.10.43",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "aws-lambda": "^1.0.5",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "serverless": "^3.25.1",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "serverless-offline": "^5.12.1"
  }
}
